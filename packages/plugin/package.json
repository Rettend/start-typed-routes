{
  "name": "start-typed-routes",
  "type": "module",
  "version": "0.1.0",
  "description": "Add strong types to SolidStart routes",
  "homepage": "https://github.com/Rettend/start-typed-routes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rettend/start-typed-routes.git"
  },
  "bugs": {
    "url": "https://github.com/Rettend/start-typed-routes/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "solid": "./dist/plugin.jsx",
      "default": "./dist/plugin.js"
    },
    "./all": {
      "types": "./dist/all.d.ts",
      "solid": "./dist/all.jsx",
      "default": "./dist/all.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "bun --bun eslint . --fix",
    "check": "tsgo --noEmit -p ./tsconfig.json",
    "release": "bumpp && npm publish",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@solidjs/router": ">=0.15.0",
    "solid-js": ">=1.9.0"
  }
}
