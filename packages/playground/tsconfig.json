{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "start-typed-routes": [
        "../plugin/src/index.ts"
      ],
      "start-typed-routes/*": [
        "../plugin/src/*"
      ],
      "start-typed-routes/plugin": [
        "../plugin/src/plugin.ts"
      ]
    },
    "types": [
      "bun",
      "node",
      "vinxi/types/client"
    ],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "app.config.ts"
  ]
}
