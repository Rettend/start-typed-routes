{
  "name": "start-typed-routes-monorepo",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --filter start-typed-routes build",
    "dev": "bun run --filter start-typed-routes dev",
    "play": "bun run --filter start-typed-routes-playground dev",
    "test": "bun run --filter start-typed-routes test",
    "lint": "bun --bun eslint . --fix",
    "release": "bun run --filter start-typed-routes release",
    "update": "taze latest -rw"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.2.0",
    "@solidjs/testing-library": "^0.8.10",
    "@types/node": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20251018.1",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "eslint-plugin-solid": "^0.14.5",
    "happy-dom": "^20.0.5",
    "rolldown-plugin-solid": "^0.2.1",
    "solid-js": "^1.9.9",
    "taze": "19.7.0",
    "tsdown": "^0.15.7",
    "vinxi": "^0.5.8",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.4"
  }
}
